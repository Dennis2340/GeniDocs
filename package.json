{
  "name": "doc-builder",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "docs:start": "cd docs && npm start",
    "docs:start:ps": "powershell -Command \"Set-Location -Path docs; npm start\"",
    "docs:build": "cd docs && npm run build",
    "docs:serve": "cd docs && npm run serve",
    "docs:build-and-serve": "cd docs && npm run build && npm run serve",
    "docs:deploy": "node scripts/deploy-docs.js",
    "docs:deploy:dev": "node scripts/deploy-docs.js --dev",
    "docs:deploy:prod": "node scripts/deploy-docs.js --prod",
    "docs:cleanup": "node docs/scripts/cleanup.js",
    "docs:fix-paths": "node docs/scripts/fix-paths.js",
    "docs:organize": "node docs/scripts/organize.js",
    "docs:rename": "node docs/scripts/rename.js",
    "docs:fix-frontmatter": "node docs/scripts/fix-frontmatter.js",
    "docs:standard": "node docs/scripts/index.js",
    "docs:update-sidebar": "node docs/scripts/auto-generate-sidebar.js",
    "docs:update-navbar": "node docs/scripts/auto-generate-navbar.js",
    "docs:generate-all": "node docs/scripts/generate-all.js"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.9.1",
    "@babel/parser": "^7.27.2",
    "@babel/traverse": "^7.27.1",
    "@octokit/rest": "^21.1.1",
    "@prisma/client": "^6.7.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "acorn": "^8.14.1",
    "acorn-jsx": "^5.3.2",
    "acorn-walk": "^8.3.4",
    "axios": "^1.9.0",
    "firebase": "^11.7.1",
    "langchain": "^0.3.24",
    "next": "15.3.2",
    "next-auth": "^4.24.11",
    "openai": "^4.98.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "tailwind-merge": "^3.3.0",
    "tree-sitter": "^0.22.4",
    "tree-sitter-css": "^0.23.2",
    "tree-sitter-html": "^0.23.2",
    "tree-sitter-javascript": "^0.23.1",
    "tree-sitter-python": "^0.23.6",
    "tree-sitter-typescript": "^0.23.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "js-yaml": "^4.1.0",
    "prisma": "^6.7.0",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.2.9",
    "typescript": "^5"
  }
}
